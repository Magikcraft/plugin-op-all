{
  "name": "@magikcraft/op-all",
  "version": "1.0.0",
  "description": "Give op to everyone who joins the server",
  "keywords": [
    "scriptcraft",
    " scriptcraft-sma",
    " plugin",
    " minecraft"
  ],
  "author": "Josh Wulf <josh@magikcraft.io>",
  "license": "ISC",
  "smaPluginConfig": {
    "scriptcraft_load_dir": "autoload"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "prettier --write",
      "tsc",
      "git add"
    ]
  },
  "devDependencies": {
    "@scriptcraft/types": "^1.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  }
}
